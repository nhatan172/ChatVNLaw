{
  "name": "chatvnlaw",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "bufferutil": "^3.0.2",
    "counterpart": "^0.18.1",
    "form-data": "^2.2.0",
    "node": "^8.3.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^15.6.1",
    "react-alert": "^2.2.0",
    "react-chat-bubble": "^0.8.6",
    "react-cookies": "^0.1.0",
    "react-dom": "^15.6.1",
    "react-fontawesome": "^1.6.1",
    "react-graph-vis": "^0.1.3",
    "react-router": "3",
    "react-scripts": "1.0.11",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.72.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/assets/styles -o src/assets/styles",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/styles -o src/assets/styles --watch --recursive",
    "start-js": "react-scripts start",
    "start": "BROWSER=none npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
